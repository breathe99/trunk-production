{
  "name": "trunk-production",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack -p && node ./bin/www",
    "serve": "concurrently \"nodemon --ignore public/ ./bin/www\" \"webpack --watch -d\" \"sass --watch public/sass/styles.scss:public/styles/styles.css\"",
    "build": "webpack -p && sass -t compressed public/sass/styles.scss:public/styles/styles.css"
  },
  "dependencies": {
    "async": "^2.0.1",
    "body-parser": "~1.12.4",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "jade": "~1.9.2",
    "jquery": "^3.1.0",
    "mongodb": "^1.4.40",
    "mongoose": "^4.4.17",
    "monk": "^1.0.1",
    "morgan": "~1.5.3",
    "node-gyp": "^3.3.1",
    "serve-favicon": "~2.2.1",
    "x-ray": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "mongoose": "^4.4.17",
    "nodemon": "^1.9.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
